{
  "version": 2,
  "functions": {
    "api/[...all].js": {
      "includeFiles": "server/**"
    }
  },
  "routes": [
    { "src": "/(sitemap\\.xml|robots\\.txt|reply|gallery-file/.*|gallery-opt/.*|api/.*)", "dest": "/api/[...all].js" },
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ]
}
